{
  "name": "i13n-client",
  "version": "0.8.7",
  "description": "i13 client",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "repository": "react-atomic/i13n",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "array.merge": "*",
    "call-func": "*",
    "css-query-selector": "*",
    "exec-script": "*",
    "get-cookie": "*",
    "get-random-id": "*",
    "i13n": "*",
    "i13n-store": "*",
    "object-nested": "*",
    "parse-ini-string": "*",
    "reshow-constant": "*",
    "seturl": "*",
    "to-percent-js": "*",
    "url-route": "*",
    "win-doc": "*"
  },
  "devDependencies": {
    "reshow-app": "*"
  },
  "scripts": {
    "clean": "find ./build ./assets -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --root-mode upward",
    "build:test": "BABEL_ENV=build babel tests -d build/tests --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mocha": "mocha -r jsdom-global/register 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
