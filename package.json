{
  "name": "i13n",
  "version": "1.5.7",
  "description": "i13n util",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "repository": "react-atomic/i13n",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "get-object-value": "*",
    "get-random-id": "*",
    "get-storage": "*",
    "reshow-constant": "*",
    "reshow-flux-base": "*",
    "reshow-runtime": "*",
    "set-object-value": "*",
    "win-doc": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "@babel/core": "^7.x",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/plugin-syntax-dynamic-import": "*",
    "@babel/plugin-transform-object-assign": "*",
    "@babel/preset-env": "*",
    "babel-plugin-reshow-transform-runtime": "*",
    "chai": "*",
    "jsdom": "*",
    "jsdom-global": "*",
    "mocha": "*",
    "prettier": "*",
    "prettier-eslint": "*",
    "prettier-eslint-cli": "*",
    "sinon": "*"
  },
  "scripts": {
    "format": "prettier-eslint --write 'src/**/*.js' 'ui/**/*.jsx'",
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src",
    "build:es": "BABEL_ENV=es babel src -d build/es/src",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha -r jsdom-global/register",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
