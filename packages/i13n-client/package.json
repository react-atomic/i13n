{
  "name": "i13n-client",
  "version": "0.12.12",
  "description": "i13 client",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "repository": "react-atomic/i13n",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "array.merge": "*",
    "array.search.js": "*",
    "call-func": "*",
    "css-query-selector": "*",
    "exec-script": "*",
    "form-serialize-js": "*",
    "get-cookie": "*",
    "get-random-id": "*",
    "html-entity-js": "*",
    "i13n": "*",
    "i13n-store": "*",
    "object-nested": "*",
    "parse-ini-string": "*",
    "reshow-constant": "*",
    "seturl": "*",
    "to-percent-js": "*",
    "url-route": "*",
    "win-doc": "*",
    "window-onload": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "reshow-app": "*"
  },
  "scripts": {
    "webpack": "webpack",
    "start": "ws",
    "clean": "find ./build ./assets -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha -r jsdom-global/register",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "nyc": "nyc npm run mocha",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test && npm run build && ./compile.sh p"
  },
  "files": [
    "assets",
    "build",
    "package.json",
    "README.md"
  ]
}
