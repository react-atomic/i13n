{
  "name": "i13n",
  "version": "0.0.18",
  "description": "i13n util",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "get-object-value": "*",
    "get-storage": "*",
    "reshow-flux-base": "*",
    "reshow-runtime": "*",
    "set-object-value": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.x",
    "babel-core": "^6.x",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-system-import-transformer": "*",
    "babel-plugin-transform-object-assign": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-env": "*",
    "babel-preset-stage-0": "*",
    "chai": "*",
    "mocha": "*",
    "sinon": "*"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src",
    "build:es": "BABEL_ENV=es babel src -d build/es/src",
    "build:test": "BABEL_ENV=build babel tests -d build/tests",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "test": "npm run build && npm run build:test && mocha 'build/tests/**/*.js'",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "store.js",
    "build",
    "package.json",
    "README.md"
  ]
}
