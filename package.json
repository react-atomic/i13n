{
  "version": "1.7.1",
  "name": "i13n",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-atomic/i13n"
  },
  "description": "i13n util",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "get-object-value": "*",
    "get-random-id": "*",
    "get-storage": "*",
    "reshow-constant": "*",
    "reshow-flux-base": "*",
    "reshow-runtime": "*",
    "set-object-value": "*",
    "win-doc": "*"
  },
  "devDependencies": {
    "react-atomic-atom": "*",
    "reshow-unit-dom": "*"
  },
  "exports": {
    "require": "./build/cjs/src/index.js",
    "import": "./build/es/src/index.mjs"
  },
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.mjs",
  "scripts": {
    "format": "prettier-eslint --write 'src/**/*.js' 'ui/**/*.jsx'",
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src --ignore /**/__tests__",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --out-file-extension .mjs",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha -r global-jsdom/register",
    "mocha": "npm run mochaFor -- 'build/es/**/__tests__/*.mjs'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
