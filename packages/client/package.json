{
  "name": "i13n-client",
  "version": "0.0.19",
  "description": "i13 client",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "i13n"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "css-query-selector": "*",
    "exec-script": "*",
    "i13n": "*",
    "i13n-store": "*",
    "object-nested": "*",
    "parse-ini-string": "*",
    "seturl": "*"
  },
  "devDependencies": {
    "babel-cli": "*",
    "reshow-app": "*"
  },
  "scripts": {
    "clean": "find ./build ./assets -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src",
    "build:es": "BABEL_ENV=es babel src -d build/es/src",
    "build:test": "BABEL_ENV=build babel tests -d build/tests",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "test": "npm run build && npm run build:test && mocha 'build/tests/**/*.js' -r jsdom-global/register",
    "prepublishOnly": "npm run test && npm run build"
  }
}
